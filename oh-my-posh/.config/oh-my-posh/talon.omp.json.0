{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "final_space": true,
  "var": {
    "ManjaroLogo": "",
    "UserIcon": "",
    "Arrow": "➤",
    "FancyArrow": "⫸",
    "Block": "▇"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "foreground_templates": [
            "{{ if eq .OS \"manjaro\" }}#35bfa4{{ else if eq .OS \"redhat\" }}#f50000{{ else if eq .OS \"centos\" }}#f50000{{ else if eq .OS \"arch\" }}#1792d1{{ else }}#FFFFFF{{ end }}"
          ],
          "style": "plain",
          "template": "<b>{{ .Icon }} </b> "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "⋖ "
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#fa6277",
          "template": "<b>{{.Var.UserIcon }} {{ .UserName }}@{{ .HostName }}:</b>"
        },
        {
          "type": "path",
          "style": "plain",
          "folder_separator_icon": "/",
          "foreground": "#8da5ff",
          "properties": {
            "style": "full"
          },
          "template": "<b>{{ .Path }}</b>"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": " ⋗"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "powerline",
          "foreground": "#45e918",
          "template": "[ ({{ .Venv }}) v{{ .Full }}] "
        },
        {
          "type": "node",
          "style": "powerline",
          "foreground": "#45e918",
          "template": "[ {{ .Full }}] "
        },
        {
          "type": "lua",
          "style": "powerline",
          "foreground": "#45e918",
          "template": "[ {{ .Full }}] "
        },
        {
          "type": "go",
          "style": "powerline",
          "foreground": "#45e918",
          "template": "[ {{ .Full }}] "
        },
        {
          "type": "rust",
          "style": "powerline",
          "foreground": "#45e918",
          "template": "[ {{ .Full }}] "
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "white",
          "background": "#9e000f",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#620511{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#838000{{ end }}",
            "{{ if gt .Ahead 0 }}#003c83{{ end }}",
            "{{ if gt .Behind 0 }}#7c0083{{ end }}"
          ],
          "template": "<b>󰊢 {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}</b>",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "status",
          "style": "powerline",
          "foreground_templates": [
            "{{ if eq .Code 0 }}#31ff00{{ else }}#ff0007{{ end }}"
          ],
          "template": "<b>{{ .Var.Arrow }}</b> ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ]
}
